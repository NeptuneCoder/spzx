<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.travel.spzx.travel.mapper.ProtocolMapper">
    <sql id="columns">
        id
        , title, content,`type`, remark,create_time,update_time, is_deleted
    </sql>
    <resultMap id="richTxtMap" type="com.travel.spzx.model.entity.richTxt.RichTxtInfo"/>
    <select id="getProtocolByName" resultMap="richTxtMap">
        select
        <include refid="columns"/>
        from desc_info
        where is_deleted = 0
        and title = #{title}
    </select>

</mapper>